{
	"ecmaFeatures": {
		"jsx": true
	},
	"env": {
		"browser": true,
		"node": true,
		"jquery": true
	},
	"rules": {
		"quotes": 0,
		"no-unused-vars": 0,
		"eol-last": 0,
		"space-return-throw-case": 0,
		"no-debugger": 0,
		"no-mixed-spaces-and-tabs": 0
	},
	"globals": {
		jQuery: true,
		$: true
	}
}